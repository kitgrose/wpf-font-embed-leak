<Application x:Class="TestWpfFontEmbedsDotNet7.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:TestWpfFontEmbedsDotNet7"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <FontFamily x:Key="Alice">./Fonts/#Alice</FontFamily>
        <FontFamily x:Key="Bagnard">./Fonts/#Bagnard</FontFamily>
        <FontFamily x:Key="CooperHewitt">./Fonts/#Cooper Hewitt</FontFamily> <!-- Loads both Light and Heavy weights -->
        <FontFamily x:Key="Inter">./Fonts/#Inter</FontFamily> <!-- Loads Light, SemiBold and Black weights -->
        <FontFamily x:Key="Oswald">./Fonts/#Oswald</FontFamily>
        <FontFamily x:Key="Pacifico">./Fonts/#Pacifico</FontFamily>
    </Application.Resources>
</Application>
